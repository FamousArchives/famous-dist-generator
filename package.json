{
  "name": "famous-dist-generator",
  "version": "0.0.2",
  "description": "Build various dist versions of famous/famous. RequireJS, CommonJS, or standalone window.famous object",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/FamousInternal/famous-dist-generator.git"
  },
  "scripts": {
    "test": "npm run-script lint; ./node_modules/.bin/tap --timeout 60 test/*.js",
    "watch": "./node_modules/.bin/nodemon --ignore node_modules/ -e js,json --exec 'npm test'",
    "lint": "./node_modules/.bin/eslint **/*.js"
  },
  "author": "Andrew de Andrade <andrew@famo.us>",
  "devDependencies": {
    "eslint": "^0.6.2",
    "nodemon": "^1.2.0",
    "tap": "^0.4.11",
    "tape": "^2.13.2",
    "temp": "^0.8.0"
  },
  "dependencies": {
    "async": "^0.9.0",
    "browserify": "^4.1.9",
    "browserify-ftw": "^0.7.0",
    "clean-css": "^2.2.2",
    "commondir": "0.0.1",
    "famous-git-cache": "^0.1.1",
    "findit": "^1.2.0",
    "handlebars": "^2.0.0-alpha.4",
    "minimist": "^0.1.0",
    "mkdirp": "^0.5.0",
    "requirejs": "^2.1.14",
    "uglify-js": "^2.4.14",
    "xtend": "^3.0.0"
  },
  "bin": {
    "famous-dist-generator": "bin/cmd.js"
  },
  "preferGlobal": "true",
  "directories": {
    "bin": "./bin",
    "lib": "./lib",
    "test": "./test"
  },
  "readme": "README.markdown",
  "engines": {
    "node": ">=0.10.28",
    "npm": ">=1.4.10"
  },
  "license": "MPLv2"
}
